{
  "version": 1,
  "projects": {
    "nx-realm": {
      "root": "packages/nx-realm",
      "sourceRoot": "packages/nx-realm/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/nx-realm/tsconfig.lib.json",
              "packages/nx-realm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/nx-realm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/nx-realm/jest.config.js",
            "tsConfig": "packages/nx-realm/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nx-realm",
            "tsConfig": "packages/nx-realm/tsconfig.lib.json",
            "packageJson": "packages/nx-realm/package.json",
            "main": "packages/nx-realm/src/index.ts",
            "assets": [
              "packages/nx-realm/*.md",
              {
                "input": "./packages/nx-realm/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-realm",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/nx-realm",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-realm-e2e": {
      "projectType": "application",
      "root": "e2e/nx-realm-e2e",
      "sourceRoot": "e2e/nx-realm-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-realm:build",
            "npmPackageName": "@trellis/nx-realm",
            "pluginOutputPath": "dist/packages/nx-realm",
            "jestConfig": "e2e/nx-realm-e2e/jest.config.js",
            "tsSpecConfig": "e2e/nx-realm-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "test8": {
      "root": "apps/test8",
      "sourceRoot": "apps/test8/src",
      "projectType": "application",
      "prefix": "test8",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "./dist/packages/nx-realm:build",
          "options": {
            "outputPath": "dist/apps/test8",
            "main": "apps/test8/src/main.ts",
            "tsConfig": "apps/test8/tsconfig.app.json",
            "assets": [
              "apps/test8/src/app/config.json",
              {
                "glob": "functions/**/*.json",
                "input": "apps/test8/src/app/",
                "output": "./app"
              },
              {
                "glob": "services/**/*.json",
                "input": "apps/test8/src/app/",
                "output": "./app"
              },
              {
                "glob": "triggers/**/*.json",
                "input": "apps/test8/src/app/",
                "output": "./app"
              },
              {
                "glob": "hosting/**/*.json",
                "input": "apps/test8/src/app/",
                "output": "./app"
              },
              {
                "glob": "values/**/*.json",
                "input": "apps/test8/src/app/",
                "output": "./app"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/test8/src/environments/environment.ts",
                  "with": "apps/test8/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "test8:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/test8/tsconfig.app.json",
              "apps/test8/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/test8/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/test8/jest.config.js",
            "tsConfig": "apps/test8/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "sales1"
}
