{
  "version": 1,
  "projects": {
    "nx-realm": {
      "root": "packages/nx-realm",
      "sourceRoot": "packages/nx-realm/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/nx-realm/tsconfig.lib.json",
              "packages/nx-realm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!packages/nx-realm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/nx-realm/jest.config.js",
            "tsConfig": "packages/nx-realm/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nx-realm",
            "tsConfig": "packages/nx-realm/tsconfig.lib.json",
            "packageJson": "packages/nx-realm/package.json",
            "main": "packages/nx-realm/src/index.ts",
            "assets": [
              "packages/nx-realm/*.md",
              {
                "input": "./packages/nx-realm/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-realm",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/nx-realm",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-realm-e2e": {
      "projectType": "application",
      "root": "e2e/nx-realm-e2e",
      "sourceRoot": "e2e/nx-realm-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-realm:build",
            "npmPackageName": "@trellis/nx-realm",
            "pluginOutputPath": "dist/packages/nx-realm",
            "jestConfig": "e2e/nx-realm-e2e/jest.config.js",
            "tsSpecConfig": "e2e/nx-realm-e2e/tsconfig.spec.json"
          }
        }
      }
    },
