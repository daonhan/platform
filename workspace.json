{
  "version": 1,
  "projects": {
    "convert-to-buildable": {
      "root": "packages/convert-to-buildable",
      "sourceRoot": "packages/convert-to-buildable/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "packages/convert-to-buildable/tsconfig.lib.json",
              "packages/convert-to-buildable/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/convert-to-buildable/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/convert-to-buildable/jest.config.js",
            "tsConfig": "packages/convert-to-buildable/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/convert-to-buildable",
            "tsConfig": "packages/convert-to-buildable/tsconfig.lib.json",
            "packageJson": "packages/convert-to-buildable/package.json",
            "main": "packages/convert-to-buildable/src/index.ts",
            "assets": [
              "packages/convert-to-buildable/*.md",
              {
                "input": "./packages/convert-to-buildable/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/convert-to-buildable",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/convert-to-buildable",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "convert-to-buildable-e2e": {
      "projectType": "application",
      "root": "e2e/convert-to-buildable-e2e",
      "sourceRoot": "e2e/convert-to-buildable-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "convert-to-buildable:build",
            "npmPackageName": "@trellis/convert-to-buildable",
            "pluginOutputPath": "dist/packages/convert-to-buildable",
            "jestConfig": "e2e/convert-to-buildable-e2e/jest.config.js",
            "tsSpecConfig": "e2e/convert-to-buildable-e2e/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
