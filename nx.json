{
  "npmScope": "trellis",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "nx-realm": {
      "tags": []
    },
    "nx-realm-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-realm"]
    },
    "make-buildable": {
      "tags": []
    },
    "make-buildable-e2e": {
      "tags": [],
      "implicitDependencies": ["make-buildable"]
    },
    "angularfire-ngrx": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  }
}
